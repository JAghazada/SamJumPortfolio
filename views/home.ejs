    </div>
    <!-- ! include card modal -->
    <%- include('partials/card.ejs') %>
        <div id="works-inp">
            <input type="text" value="<%=works%>">
        </div>
        <!-- ! header start -->
        <!-- ? side-navbar -->
        <div class="side-bar-icon">
            <img src="/icons/navbar-icon.svg" alt="navbar-icon">
        </div>
        <div class="side-bar">
            <ul id="side-menu">
                <li class="#main-side active">Main</li>
                <li class="#about-side">About</li>
                <li class="#works-side">Works</li>
                <li class="#contact-side">Contact</li>
            </ul>
        </div>
        <header>
            <nav>
                <ul id="menu">
                    <li class="nav-item #main active">Main</li>
                    <li class="nav-item #about">About</li>
                    <li class="nav-item #works">Works</li>
                    <li class="nav-item #contact">Contact</li>
                </ul>
            </nav>
        </header>
        <!-- ! header end -->

        <!-- ! introduction start -->
        <section class="introduction-wrapper container" id="main">
            <div class="introduction-texts">
                <div class="introduction-main-text ">
                    <span>Hello, I am</span> <br>
                    <span id="name">Samir Jumshudzada</span>
                </div>
                <div class="description mt-24">
                    <%=pageData.texts.motto%> <img src="/icons/smile.svg" class="smile-icon" alt="smile icon">
                </div>
                <div class="introduction-btns mt-24">
                    <button class="btn say-hi-btn">
                        Say Hi ðŸ‘‹
                    </button>
                    <button class="btn discover-works-btn">
                        Discover my works <i class="fa-solid fa-arrow-down"></i>
                    </button>
                </div>
            </div>
            <div class="introduction-img">
                <img id="samjumphoto" src="/images/SamJumCoolBest.png" alt="samjum photo">
            </div>
        </section>
        <!-- !introduction end -->


        <!-- !About start -->
        <section id="about">
            <div class="about-wrapper container">
                <div class="header-text">
                    <%=pageData.sectionHeaders.about_section%>
                        <ul id="about">
                            <li class="about-item ab-item-1">Started my first journey at a university start-up as UX
                                designer at
                                the
                                beginning
                                of 2021 <img src="/icons/icon1.svg" alt="icon"></li>
                            <li class="about-item ab-item-2">Led the design team of a winner start-up at the same
                                year<img src="/icons/icon2.svg" alt="icon"></li>
                            <li class="about-item ab-item-3">Got my degree in UX/UI design at 2022. Finished all 7 steps
                                of
                                Googleâ€™s
                                UX design
                                course at 2023<img src="/icons/icon3.svg" alt="icon"></li>
                            <li class="about-item ab-item-4">Won several contests about UX/UI design<img
                                    src="/icons/icon4.svg" alt="icon"></li>
                            <li class="about-item ab-item-5">During this time, got different freelance UX and UI jobs
                                from
                                different
                                companies
                                <img src="/icons/icon5.svg" alt="icon">
                            </li>
                        </ul>
                </div>

            </div>
        </section>

        <!-- !About end -->
        <!-- !Works start -->
        <div class="works-wrapper">
            <section class="header-text container" id="works">
                <%=pageData.sectionHeaders.work_section%>
            </section>
            <div class="works-img-container">
                <div class="header-img">
                    <button class="view-details-btn" data-id="<%=works[4].projectID%>">View Details</button>
                    <img src="/images/projects/project-1.png" alt="project-image">

                </div>
                <div class="inner-imgs">
                    <div class="left-imgs">
                        <div>
                            <button class="view-details-btn" data-id="<%=works[6].projectID%>">View Details</button>
                            <img src="/images/projects/project-3.png" alt="project-image">
                        </div>
                        <div>
                            <button class="view-details-btn" data-id="<%=works[5].projectID%>">View Details</button>
                            <img src="/images/projects/project-2.png" alt="project-image">
                        </div>
                    </div>
                    <div class="right-imgs">
                        <div>
                            <button class="view-details-btn" data-id="<%=works[3].projectID%>">View Details</button>
                            <img src="/images/projects/project-4.png" alt="project-image">
                        </div>
                        <div>
                            <button class="view-details-btn" data-id="<%=works[2].projectID%>">View Details</button>
                            <img src="/images/projects/project-6.png" alt="project-image">
                        </div>
                        <div>
                            <button class="view-details-btn" data-id="<%=works[1].projectID%>">View Details</button>
                            <img src="/images/projects/project-5.png" alt="project-image">
                        </div>


                    </div>
                </div>
                <div class="footer-img">
                    <button class="view-details-btn" data-id="<%=works[0].projectID%>">View Details</button>
                    <img src="/images/projects/project-7.png" alt="project-image">

                </div>
            </div>
        </div>
        <div class="w-100 el-center">
            <button class="btn show-more-btn">
                <a href="/works">Show more
                    <svg width="16" height="14" viewBox="0 0 16 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path
                            d="M0.170898 7.09131C0.170898 6.65967 0.469727 6.35254 0.901367 6.35254L11.8335 6.35254L13.6016 6.41064L10.8789 3.92871L9.01953 2.03613C8.88672 1.90332 8.82031 1.7124 8.82031 1.51318C8.82031 1.09814 9.13574 0.799316 9.54248 0.799316C9.7417 0.799316 9.91602 0.865723 10.082 1.02344L15.6021 6.53516C15.7681 6.69287 15.8511 6.88379 15.8511 7.09131C15.8511 7.29883 15.7681 7.48975 15.6021 7.64746L10.082 13.1509C9.91602 13.3086 9.7417 13.3833 9.54248 13.3833C9.13574 13.3833 8.82031 13.0845 8.82031 12.6694C8.82031 12.4702 8.88672 12.271 9.01953 12.1465L10.8789 10.2539L13.6099 7.76367L11.8335 7.83008L0.901367 7.83008C0.469727 7.83008 0.170898 7.52295 0.170898 7.09131Z"
                            fill="" fill-opacity="0.87" />
                </a>
                </svg>

            </button>
        </div>
        <!-- !Works end  -->
        <!-- !Contact start  -->
        <section class="contact-wrapper container" id="contact">
            <div class="header-text">
                <%=pageData.sectionHeaders.contact_section%>
            </div>
            <div class="contact-container">
                <div class="contact-inputs">
                    <input type="text" placeholder="name" id="name-inp">
                    <input type="text" placeholder="surname" id="surname-inp">
                </div>
                <div class="contact-inputs">
                    <input type="text" placeholder="Email" id="email-inp">
                    <input type="text" placeholder="Phone Number" id="phone-inp">

                </div>
                <div class="contact-inputs">
                    <input type="text" placeholder="Message..." id="message-inp">
                    <button class="send-message-btn">
                        Send <i class="fa-regular fa-share-from-square"></i>
                    </button>
                </div>

            </div>
        </section>

        <!-- !Contact end -->
        <!-- !footer start -->
        <footer class="container">
            <div class="footer-tags">
                <div class="footer-tag">
                    <div class="footer-header">
                        <%=pageData.footerHeaders.footer_works%>
                            <ul id="footer-menu">
                                <li class="footer-item"><a href="<%=pageData.social_links.behance%>">Behance</a></li>
                                <li class="footer-item"><a href="<%=pageData.social_links.drible%>">Drible</a></li>
                                <li class="footer-item"><a href="<%=pageData.social_links.pinterest%>">Pinterest</a>
                                </li>
                            </ul>
                    </div>


                </div>
                <div class="footer-tag" id="social-links">
                    <div class="footer-header">
                        <%=pageData.footerHeaders.footer_social%>
                            <ul id="footer-menu">
                                <li class="footer-item"><a href="<%=pageData.social_links.instagram%>">Instagram</a>
                                </li>
                                <li class="footer-item"><a href="<%=pageData.social_links.linkedin%>">Linkedin</a></li>
                                <li class="footer-item"><a href="<%=pageData.social_links.facebook%>">Facebook</a></li>
                                <li class="footer-item"><a href="<%=pageData.social_links.twitter%>">Twitter</a></li>
                            </ul>
                    </div>
                </div>
                <div class="footer-tag">
                    <div class="footer-header">
                        <%=pageData.footerHeaders.footer_contact%>
                            <ul id="footer-menu">
                                <li class="footer-item"><i class="fa-regular fa-envelope"></i>
                                    <%=pageData.social_links.gmail%>
                                </li>
                                <li class="footer-item"><i class="fa-solid fa-phone-volume"></i>
                                    <%=pageData.social_links.phone%>
                                </li>
                                <li class="footer-item">
                                    <%=pageData.texts.contact_text%>
                                </li>
                            </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!-- !footer end -->
        <div class="copyright container">
            Â©2023 Samir Jumshudzada. All Rights Reserved.
        </div>


        <!-- ? font awesome script -->
        <script src="https://kit.fontawesome.com/46a0505f50.js" crossorigin="anonymous"></script>
        <!-- ? My Scripts -->
        <script src="/js/scripts/changeImg.js"></script>
        <script src="/js/scripts/side-bar.js"></script>
        <script src="/js/scripts/nav-scroll.js"></script>

        <script>
            const works = '<%- JSON.stringify(works) %>'
        </script>

        <script src="/js/scripts/about-projects.js"></script>
        <script src="/js/scripts/send-mail.js"></script>
        <script src="/js/scripts/img.script.js"></script>


</body>

</html>